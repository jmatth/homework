program main;
var a, b: integer;
    i: integer;
    c: boolean;
    d: array[1] of boolean;
begin
  i := 0;
  a := 1;
  b := 10;
  c := 5;
  while d[i] < 100 do
  begin
    a := c + 2;
    b := b[5] + 1;
    c := a and true;
    d[false] := true;
    d := false
  end	
  endwhile;
  print(i);
  print(a);
  print(b)
end.
